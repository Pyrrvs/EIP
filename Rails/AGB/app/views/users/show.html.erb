<div id="user-profile">
	<p>
	  <h3>User profile</h3>
	  <b>Name:</b>
	  <%= @user.name %>
	</p>

	<p>
	  <b>Email:</b>
	  <%= @user.email %>
	</p>

	<%= link_to 'Destroy User', user_path(@user), method: :delete, confirm: "Are you sure?" %>
</div>
<div id="user-projects">
	<p>
		<%= render 'projects/index' %>
	</p>
</div>
